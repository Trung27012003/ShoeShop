function START() 
menu = gg.choice({  '1. Hack skip',
                    '2. Hack Level',
                    '3. Support đua Heroic',
                    '4. Move Habit',
                    '❎ Exit ❎'},Last,
                    '--------------------------------------Trung2k3dz-------------------------------------')
